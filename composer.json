{
  "name": "neontribe/arcvservice",
  "description": "ARCV Service",
  "version": "1.12.0",
  "keywords": [],
  "license": "proprietary",
  "type": "project",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/neontribe/laravel-specification.git"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-sodium": "*",
    "ext-zip": "*",
    "barryvdh/laravel-debugbar": "^3.6.7",
    "barryvdh/laravel-dompdf": "^0.8.1",
    "doctrine/dbal": "^2.5",
    "fakerphp/faker": "^1.19",
    "fruitcake/laravel-cors": "2.0.5",
    "laravel/framework": "^6.19",
    "laravel/helpers": "^1.5.0",
    "laravel/passport": "^9.3.0",
    "laravel/tinker": "^2.7.0",
    "maennchen/zipstream-php": "^2.0",
    "moontoast/math": "^1.1",
    "neontribe/laravel-specification": "dev-master",
    "ramsey/uuid": "^3.7",
    "sebdesign/laravel-state-machine": "^2.0",
    "symfony/event-dispatcher": "^4.4.37",
    "tom-lingham/searchy": "2.*",
    "usmanhalalit/laracsv": "^2.0"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.8.2",
    "filp/whoops": "~2.14.5",
    "laracasts/generators": "^1.1",
    "laravel/browser-kit-testing": "~5.0",
    "mockery/mockery": "^1.3.5",
    "nunomaduro/collision": "^3.0",
    "phpunit/phpunit": "^8.5.23",
    "spinen/laravel-mail-assertions": "^1.0"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "app/",
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.2.22"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  }
}
