{
  "name": "neontribe/arcvservice",
  "description": "ARCV Service",
  "version": "1.14.1",
  "keywords": [],
  "license": "proprietary",
  "type": "project",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/neontribe/laravel-specification.git"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-sodium": "*",
    "ext-zip": "*",
    "barryvdh/laravel-debugbar": "^v3.6.7",
    "barryvdh/laravel-dompdf": "2.0.0",
    "doctrine/dbal": "3.5.2",
    "fakerphp/faker": "^1.19.0",
    "fruitcake/laravel-cors": "3.0.0",
    "laravel/framework": "^9.0",
    "laravel/helpers": "^1.5.0",
    "laravel/legacy-factories": "^1.3",
    "laravel/passport": "11.4.0",
    "laravel/tinker": "^2.7.2",
    "laravel/ui": "^4.1",
    "maennchen/zipstream-php": "^2.1.0",
    "neontribe/laravel-specification": "dev-master",
    "ramsey/uuid": "4.7.1",
    "sebdesign/laravel-state-machine": "3.3.0",
    "symfony/console": "6.2.3",
    "symfony/event-dispatcher": "6.2.2",
    "usmanhalalit/laracsv": "^2.1.0"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.8.2",
    "filp/whoops": "~2.14.5",
    "laracasts/generators": "^2.0.1",
    "laravel/browser-kit-testing": "6.3.1",
    "mockery/mockery": "^1.3.5",
    "nunomaduro/collision": "6.4.0",
    "phpunit/phpunit": "9.5.27",
    "phpmd/phpmd": "^2.12.0",
    "squizlabs/php_codesniffer": "^3.6.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "app/",
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.1"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  }
}
