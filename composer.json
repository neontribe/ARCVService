{
    "name": "neontribe/arcvservice",
    "description": "ARCV Service",
    "version": "1.7.0-dev",
    "keywords": [],
    "license": "",
    "type": "project",
    "require": {
        "barryvdh/laravel-cors": "^0.11",
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-dompdf": "^0.8.1",
        "doctrine/dbal": "^2.5",
        "laravel/framework": "5.5.*",
        "laravel/passport": "^3.0",
        "laravel/tinker": "~1.0",
        "maatwebsite/excel": "^2.1",
        "maennchen/zipstream-php": "0.5.2",
        "moontoast/math": "^1.1",
        "optimus/api-consumer": "0.2.*",
        "ramsey/uuid": "^3.6",
        "sebdesign/laravel-state-machine": "^1.1",
        "symfony/event-dispatcher" : "4.2.11",
        "tom-lingham/searchy": "2.*"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4",
        "fzaninotto/faker": "~1.4",
        "filp/whoops": "~2.0",
        "laravel/dusk": "^2.0",
        "laracasts/generators": "^1.1.4",
        "laravel/browser-kit-testing": "2.*",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "spinen/laravel-mail-assertions": "^0.3.0"
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php" : "7.2"
        }
    }
}
